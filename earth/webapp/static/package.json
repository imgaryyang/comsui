    "element-theme": "0.2.0",
    "element-theme-default": "1.0.9",
    "eslint": "3.13.1",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "gulp": "3.9.1",
    "gulp-babel": "6.1.2",
    "gulp-clean": "0.3.2",

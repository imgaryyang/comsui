{
  "name": "build",
  "version": "0.0.1",
  "description": "build js/css",
  "scripts": {
    "dev": "cross-env NODE_ENV=development DEPLOY_PRODUCTION=yunxin webpack-dev-server --config webpack.config.dev.js --content-base ../ --inline --hot --port 8089 --host 0.0.0.0",
    "dist": "gulp deploy && cross-env NODE_ENV=production DEPLOY_PRODUCTION=yunxin webpack --config webpack.config.dist.js --progress --hide-modules && gulp dist:replace",
    "dev_yunxin": "npm run dev",
    "dist_yunxin": "npm run dist",
    "dev_shrbank": "cross-env NODE_ENV=development DEPLOY_PRODUCTION=shrbank webpack-dev-server --config webpack.config.dev.js --content-base ../ --inline --hot",
    "dist_shrbank": "gulp deploy && cross-env NODE_ENV=production DEPLOY_PRODUCTION=shrbank webpack --config webpack.config.dist.js --progress --hide-modules && gulp dist:replace",
    "dev_avictc": "cross-env NODE_ENV=development DEPLOY_PRODUCTION=avictc webpack-dev-server --config webpack.config.dev.js --content-base ../ --inline --hot",
    "dist_avictc": "gulp deploy && cross-env NODE_ENV=production DEPLOY_PRODUCTION=avictc webpack --config webpack.config.dist.js --progress --hide-modules && gulp dist:replace",
    "dev_weifang": "cross-env NODE_ENV=development DEPLOY_PRODUCTION=weifang webpack-dev-server --config webpack.config.dev.js --content-base ../ --inline --hot",
    "dist_weifang": "gulp deploy && cross-env NODE_ENV=production DEPLOY_PRODUCTION=weifang webpack --config webpack.config.dist.js --progress --hide-modules && gulp dist:replace",
    "dev_bohai": "cross-env NODE_ENV=development DEPLOY_PRODUCTION=bohai webpack-dev-server --config webpack.config.dev.js --content-base ../ --inline --hot",
    "dist_bohai": "gulp deploy && cross-env NODE_ENV=production DEPLOY_PRODUCTION=bohai webpack --config webpack.config.dist.js --progress --hide-modules && gulp dist:replace",
    "theme": "node_modules/.bin/et -c src/assets/themes/element-variables.css -o src/assets/themes/default",
    "make": "cross-env NODE_ENV=production webpack --config webpack.config.dist.js --progress --hide-modules && gulp dist:replace"
  },
